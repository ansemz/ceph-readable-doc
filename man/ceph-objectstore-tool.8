.nf
.ft C
[root@osd ~]# ceph\-objectstore\-tool \-\-data\-path /var/lib/ceph/osd/ceph\-0 \-\-pgid 0.1c \(aq{"oid":"zone_info.default","key":"","snapid":\-2,"hash":235010478,"max":0,"pool":11,"namespace":""}\(aq remove
.ft P
.fi
